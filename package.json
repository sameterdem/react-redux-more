{
  "name": "react-redux-more",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "2.0.0-next.3e165448",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "yarn run lint && yarn test && yarn run coverage",
    "coverage": "yarn run test -- --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/index.js",
    "postcoverage": "istanbul check-coverage --statements 80 --functions 80 --branches 80 --lines 80",
    "eject": "react-scripts eject",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint 'src/**/*.css' && stylelint 'src/**/*.sass'; exit 0",
    "lint": "yarn run lint:stylelint && yarn run lint:eslint",
    "format": "prettier --print-width=100 --tab-width=2 --use-tabs=false --semi --single-quote --bracket-spacing --trailing-comma=es5 --write \"src/**/*.js\"",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "yarn run format",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "devDependencies": {
    "codecov": "^3.0.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "istanbul": "^0.4.5",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.7",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
